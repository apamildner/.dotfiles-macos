# Add homebrew to path if not already added
if [[ ":$PATH:" != *":/opt/homebrew/bin:"* ]]; then
  export PATH="/opt/homebrew/bin:$PATH"
fi

# Add custom scripts folder to path if not already added
if [[ ":$PATH:" != *":$DOTFILES/scripts:"* ]]; then
  export PATH="$DOTFILES/scripts:$PATH"
fi

 # Add homebrew to path if not already added
if [[ ":$PATH:" != *": /Users/arvid/.local/bin:"* ]]; then
  export PATH=" /Users/arvid/.local/bin:$PATH"
fi

export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"

export PATH="/opt/homebrew/opt/openjdk@21/bin:$HOME/.cargo/bin:$PATH"
